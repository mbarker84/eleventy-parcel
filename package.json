{
  "name": "eleventy-parcel",
  "version": "2.2.0",
  "description": "",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "source": [
    "src/css/styles.scss",
    "src/js/index.js"
  ],
  "scripts": {
    "start": "run-s start:remove build:images start:watch",
    "start:remove": "node onStart.mjs",
    "start:watch": "run-p watch:eleventy",
    "watch:eleventy": "npx @11ty/eleventy --serve --incremental",
    "watch:parcel": "parcel watch --no-cache",
    "build": "run-s clean build:parcel build:images hash build:eleventy",
    "svg": "svg-symbol-sprite -i src/icons -o src/_includes/partials/sprite.svg -p ''",
    "build:eleventy": "npx @11ty/eleventy",
    "build:parcel": "parcel build --no-cache",
    "build:images": "parcel build 'src/images/*'",
    "clean": "rm -rf dist",
    "hash": "node onBuild.mjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "@11ty/eleventy-server-browsersync": "^1.0.2",
    "@parcel/transformer-sass": "^2.11.0",
    "@parcel/transformer-webmanifest": "^2.11.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "md5": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.11.0",
    "prettier": "^3.2.4",
    "process": "^0.11.10",
    "sass": "^1.70.0",
    "stylelint": "^16.2.0",
    "svg-symbol-sprite": "^1.4.1"
  },
  "dependencies": {}
}
